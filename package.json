{
	"name": "vue3-fullpage",
	"version": "0.2.0",
	"author": "southcoastweb",
	"homepage": "https://southcoastweb.co.uk",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/southcoastweb/vue3-fullpage.git"
	},
	"scripts": {
		"dev": "vite",
		"build": "vite build && tsc --emitDeclarationOnly",
		"test:unit": "vitest --dom",
		"lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
		"format": "prettier --write src/",
		"prepublishOnly": "npm run lint && npm run build"
	},
	"description": "",
	"main": "./dist/vue3-fullpage.cjs",
	"module": "./dist/vue3-fullpage.js",
	"browser": "./dist/vue3-fullpage.umd.cjs",
	"type": "module",
	"types": "./dist/types/index.d.ts",
	"exports": {
		".": {
			"import": "./dist/vue3-fullpage.js",
			"require": "./dist/vue3-fullpage.cjs",
			"browser": "./dist/vue3-fullpage.umd.cjs",
			"types": "./dist/types/index.d.ts"
		},
		"./styles": "./dist/style.css"
	},
	"files": [
		"dist"
	],
	"keywords": [
		"vue",
		"vue3",
		"fullpage",
		"scroll",
		"page"
	],
	"license": "MIT",
	"peerDependencies": {
		"vue": "^3.2.0"
	},
	"devDependencies": {
		"@rushstack/eslint-patch": "^1.2.0",
		"@testing-library/vue": "^6.6.1",
		"@types/jsdom": "^21.1.0",
		"@types/node": "^18.15.11",
		"@vitejs/plugin-vue": "^4.0.0",
		"@vue/eslint-config-prettier": "^7.1.0",
		"@vue/eslint-config-typescript": "^11.0.2",
		"@vue/test-utils": "^2.3.0",
		"@vue/tsconfig": "^0.1.3",
		"@vueuse/core": "^10.0.2",
		"autoprefixer": "^10.4.14",
		"eslint": "^8.34.0",
		"eslint-plugin-vue": "^9.9.0",
		"happy-dom": "^8.9.0",
		"jsdom": "^21.1.0",
		"lodash-es": "^4.17.21",
		"postcss": "^8.4.21",
		"postcss-import": "^15.1.0",
		"postcss-nested": "^6.0.1",
		"postcss-nesting": "^11.2.2",
		"prettier": "^2.8.4",
		"typescript": "~4.7.4",
		"vite": "^4.1.3",
		"vite-plugin-dts": "^2.0.2",
		"vitest": "^0.29.1",
		"vue-tsc": "^1.1.5"
	},
	"browserslist": [
		"> 1%",
		"last 2 versions",
		"not dead"
	]
}
